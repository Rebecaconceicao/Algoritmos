algoritmo "ex-006"
//Faça um programa que leia um número inteiro e
//mostre o seu antecessor e seu sucessor
var
      num,ant,suc: inteiro
inicio
       Escreva("Qual o numero? ")
       leia(num)
       ant <- num -1
       suc <- num +1
       Escreva("Ok! o antecessor é ",ant," e o sucessor é ",suc," tudo certo??")
fimalgoritmo